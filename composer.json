{
  "name": "fond-of-spryker/google-custom-search",
  "description": "Implements Google Search into the Spryker Suite",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "pascal.fischer@fondof.de"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "spryker/glossary": "^3.8.0",
    "spryker/zed-request": "^3.12.0",
    "fond-of-php/google-custom-search": "^2.1.1"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "tests/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
